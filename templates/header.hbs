<script type="text/javascript">
  jQuery(document).ready(function () {
    let searchParams = new URLSearchParams(window.location.search)
    if(searchParams.has('embed') || document.cookie.split(';').some((item) => item.trim().startsWith('embed='))) {
      if(!document.cookie.split(';').some((item) => item.trim().startsWith('embed='))){
